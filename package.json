  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@linthtml/linthtml": "^0.5.0",
    "@mate-academy/linthtml-config": "^0.0.1",
    "@mate-academy/stylelint-config": "^0.0.9",
    "concurrently": "^5.3.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "node-sass": "^4.14.1",
    "stylelint": "^13.7.2"
  },
  "husky": {
    "hooks": {
